#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/kaiser/kuber/landing-svelte/node_modules/.pnpm/@sveltejs+kit@2.5.10_@sveltejs+vite-plugin-svelte@3.1.1_svelte@4.2.17_vite@5.2.12_@types+node_webywwft5idj6hos5zmntq6xxm/node_modules/@sveltejs/kit/node_modules:/home/kaiser/kuber/landing-svelte/node_modules/.pnpm/@sveltejs+kit@2.5.10_@sveltejs+vite-plugin-svelte@3.1.1_svelte@4.2.17_vite@5.2.12_@types+node_webywwft5idj6hos5zmntq6xxm/node_modules/@sveltejs/node_modules:/home/kaiser/kuber/landing-svelte/node_modules/.pnpm/@sveltejs+kit@2.5.10_@sveltejs+vite-plugin-svelte@3.1.1_svelte@4.2.17_vite@5.2.12_@types+node_webywwft5idj6hos5zmntq6xxm/node_modules:/home/kaiser/kuber/landing-svelte/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/kaiser/kuber/landing-svelte/node_modules/.pnpm/@sveltejs+kit@2.5.10_@sveltejs+vite-plugin-svelte@3.1.1_svelte@4.2.17_vite@5.2.12_@types+node_webywwft5idj6hos5zmntq6xxm/node_modules/@sveltejs/kit/node_modules:/home/kaiser/kuber/landing-svelte/node_modules/.pnpm/@sveltejs+kit@2.5.10_@sveltejs+vite-plugin-svelte@3.1.1_svelte@4.2.17_vite@5.2.12_@types+node_webywwft5idj6hos5zmntq6xxm/node_modules/@sveltejs/node_modules:/home/kaiser/kuber/landing-svelte/node_modules/.pnpm/@sveltejs+kit@2.5.10_@sveltejs+vite-plugin-svelte@3.1.1_svelte@4.2.17_vite@5.2.12_@types+node_webywwft5idj6hos5zmntq6xxm/node_modules:/home/kaiser/kuber/landing-svelte/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@sveltejs/kit/svelte-kit.js" "$@"
else
  exec node  "$basedir/../@sveltejs/kit/svelte-kit.js" "$@"
fi
